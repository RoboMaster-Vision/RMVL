if(NOT TARGET @PKG_MODULE_CONFIGCMAKE@)
  add_library(@PKG_MODULE_CONFIGCMAKE@ SHARED IMPORTED)
  if(WIN32)
    set_target_properties(@PKG_MODULE_CONFIGCMAKE@ PROPERTIES
      IMPORTED_IMPLIB "@PKG_WINIMPLIB_CONFIGCMAKE@"
      IMPORTED_LOCATION "@PKG_WINLOCATION_CONFIGCMAKE@"
      INTERFACE_INCLUDE_DIRECTORIES "@PKG_INCLUDE_CONFIGCMAKE@"
    )
  else()
    set_target_properties(@PKG_MODULE_CONFIGCMAKE@ PROPERTIES
      IMPORTED_LOCATION "@PKG_LOCATION_CONFIGCMAKE@"
      INTERFACE_INCLUDE_DIRECTORIES "@PKG_INCLUDE_CONFIGCMAKE@"
    )
  endif()
endif()
